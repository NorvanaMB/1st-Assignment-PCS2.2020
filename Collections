#To measure the length of the String (len() built in collection)
def string_length(str):
    count = 0
    for char in str:
        count += 1
    return count
print(string_length("Bioinformatics"))

2#To get a leap year
def is_leap(year):
    leap = False
    if year%400==0:
        leap = True
    elif year%100==0:
        leap = False
    elif year%4==0:
        leap = True
    return leap

year = int(input())
print(is_leap(year))


